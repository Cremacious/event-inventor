
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

   model Event {
      id          String       @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
      name        String
      type    String
      images      String[]
      date       String
      visibility String
      description String
      createdAt   DateTime     @default(now()) @db.Timestamp(6)
    }